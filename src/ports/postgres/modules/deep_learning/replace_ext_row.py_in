import sys
from os import path
import os

lines = sys.stdin.readlines()
lines = [ line.strip() for line in lines ]

log_fd = open('/tmp/replace.log', 'a')

for line in lines:
    primary_key, _ = line.split(',', 1)
    fname = "/tmp/" + sys.argv[1] + primary_key
    fd = open(fname, 'w')
    fd.write(line)
    fd.close()
    log_fd.write(line)
    log_fd.write("\n---------\n")

log_fd.write("\n=========\n")
log_fd.close()
